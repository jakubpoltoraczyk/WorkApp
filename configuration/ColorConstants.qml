pragma Singleton
import QtQuick 2.12

QtObject {
    /** Black color used in application */
    readonly property color black: "black"

    /** White color used in application */
    readonly property color white: "white"

    /** Extra light grey color used in application */
    readonly property color extraLightGrey: "#dddddd"

    /** Light grey color used in application */
    readonly property color lightGrey: "#c0c0c0"

    /** Medium dark grey color used in application */
    readonly property color mediumDarkGrey: "#888888"

    /** Dark grey color used in application */
    readonly property color darkGrey: "#606060"

    /** Transparent color used in application */
    readonly property color transparent: "transparent"
}
